##
## importing module
##
import bofh;

##
## defining some custom error page subroutines
##
sub bofh_error {
	# custom error page layout
	set beresp.http.Content-Type = "application/xhtml+xml; charset=utf-8";
	synthetic({"<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
	<head>
		<title>"} + beresp.status + " " + beresp.reason + {"</title>
		<style type="text/css" rel="stylesheet">/*<![CDATA[*/
			html,body {height:100%;color:#0e9;} body {background:#000;overflow:hidden;margin:0;} #error {height:0;width:0;margin:0 auto;position:relative;top:50%;z-index:-1;} #page {margin:0;padding:20px;} #sysfail {position:absolute;background:#000 center no-repeat;margin:-45px 0 0 -400px;height:90px;width:800px;} h1 {color:#f22;margin:0;} #footer {position:fixed;bottom:20px;right:20px;left:20px;height:70px;text-align:right;} #footer p {float:left;} #footer address {float:right;} #footer address a {display:block;height:64px;width:235px;} #footer address a span {display:none;} #sysfail { background:url(data:image/jpeg;base64,); } #footer address a { background:url(data:image/gif;base64,); }
		/*]]>*/</style>
	</head>
	<body>
		<div id="error"><div id="sysfail"></div></div>
		<div id="page">
			<h1>Error "} + beresp.status + " " + beresp.reason + {"</h1>
			<h3><em>"} + bofh.excuse() + {".</em></h3>
			<p>Guru Meditation</p>
		</div>
		<div id="footer">
			<p>Transaction (XID): "} + bereq.xid + {"</p>
			<address><a href="http://www.varnish-cache.org/"><span>Varnish cache server</span></a></address>
		</div>
	</body>
</html>
"});

}

sub synth_bofh_error {
	# custom error page layout
	# since Varnish split 'obj' into 'resp' and 'beresp', we need two different versions :(
	set resp.http.Content-Type = "application/xhtml+xml; charset=utf-8";
	synthetic({"<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
	<head>
		<title>"} + resp.status + " " + resp.reason + {"</title>
		<style type="text/css" rel="stylesheet">/*<![CDATA[*/
			html,body {height:100%;color:#0e9;} body {background:#000;overflow:hidden;margin:0;} #error {height:0;width:0;margin:0 auto;position:relative;top:50%;z-index:-1;} #page {margin:0;padding:20px;} #sysfail {position:absolute;background:#000 center no-repeat;margin:-45px 0 0 -400px;height:90px;width:800px;} h1 {color:#f22;margin:0;} #footer {position:fixed;bottom:20px;right:20px;left:20px;height:70px;text-align:right;} #footer p {float:left;} #footer address {float:right;} #footer address a {display:block;height:64px;width:235px;} #footer address a span {display:none;} #sysfail { background:url(data:image/jpeg;base64,); } #footer address a { background:url(data:image/gif;base64,); }
		/*]]>*/</style>
	</head>
	<body>
		<div id="error"><div id="sysfail"></div></div>
		<div id="page">
			<h1>Error "} + resp.status + " " + resp.reason + {"</h1>
			<h3><em>"} + bofh.excuse() + {".</em></h3>
			<p>Guru Meditation</p>
		</div>
		<div id="footer">
			<p>Transaction (XID): "} + req.xid + {"</p>
			<address><a href="http://www.varnish-cache.org/"><span>Varnish cache server</span></a></address>
		</div>
	</body>
</html>
"});

}

##
## calling examples
##
## a) customized (backend)
sub vcl_backend_error {
	if (false) { # not enabled by default, but make compiler compile
		call bofh_error;
		return (deliver);
	}
}

## b) synthetic (generated)
sub vcl_synth {
	# 2nd. elaborate variant using custom page
	if (resp.status == 750) {
		set resp.status = 200;
		set resp.reason = bofh.excuse();
	}

	# 3rd. even more elaborate variant
	# passing on the excuse number
	if (resp.reason == "BOFH") {
		if (resp.status != 200) {
                        # `resp.status` only accepts valid status numbers (>=100?),
                        # so we just work around that by subtracting a count of 100
                        # (0 is valid, negative numbers are not).
			bofh.fix_excuse(resp.status - 100);

			# reset to "real" response status
			set resp.status = 200;
		}
		set resp.reason = bofh.excuse();
	}

	# 4th. custom synth page with `bofh.excuse()` call included
	if (resp.reason == "custom-page") {
		set resp.reason = 0; # reset status reason
		set resp.status = resp.status; # reset status to have varnish fill in the reason
		# bofh.excuse() is called from inside synthetic() here
		call synth_bofh_error;
		return (deliver);
	}
}
